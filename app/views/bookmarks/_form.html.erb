<h1 class="form-text">Add a new bookmark</h1>

<div class="form-container">
  <div class="container">
    <%= simple_form_for [@list, @bookmark] do |f|  %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.submit "Submit bookmark", class:"btn btn-primary" %>
    <% end %>
  </div>
</div>
